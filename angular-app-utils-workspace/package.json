{
  "name": "angular-app-utils-workspace",
  "version": "1.0.1",
  "repository": {
    "url": "https://github.com/Mind-Informatica-srl/angularapputils"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build_lib_prod": "ng build angular-app-utils-lib --configuration production",
